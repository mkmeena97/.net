@{
}
@model List<PersonCradit>
<h1>Person Info</h1>
<table class="table table-sm table-bordered table-striped" >
    <thead>
        <tr>
            <td>Pid</td>
            <td>Fname</td>
            <td>Lname</td>
            <td>Cardno</td>
            <td>BankName</td>
            <td>ExpiryDate</td>
        </tr>
    </thead>
    <tbody>
    @foreach(var p in Model)
    {
        <tr>
            <td> @p.Pid</td>
            <td> @p.Fname</td>
            <td> @p.Lname</td>
            <td> @p.Cardno</td>
            <td> @p.Bankname</td>
            <td> @p.Expirydate</td>
        </tr>
    }
    </tbody>
</table>
